# Dashboard Estoque TI - Configurações de Ambiente
# Copie este arquivo para .env e configure as variáveis

# === AUTENTICAÇÃO ===
ADMIN_PASSWORD=admin123_CHANGE_ME
VIEWER_PASSWORD=view123_CHANGE_ME

# === SEGURANÇA ===
SECRET_KEY=your-secret-key-here-CHANGE-ME
JWT_SECRET=your-jwt-secret-here-CHANGE-ME

# === BANCO DE DADOS ===
DATABASE_URL=sqlite:///estoque_ti.db
# Para PostgreSQL: postgresql://user:password@localhost:5432/estoque_ti

# === CONFIGURAÇÕES GERAIS ===
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO

# === RATE LIMITING ===
MAX_REQUESTS_PER_MINUTE=30
MAX_LOGIN_ATTEMPTS=5

# === CACHE ===
CACHE_TTL_SECONDS=300
ENABLE_CACHE=true
